# Docker 환경용 환경 변수 설정

# 데이터베이스 설정
DB_HOST=postgres
DB_PORT=5432
DB_USER=dbagentuser
DB_PASSWORD=supersecret
DB_NAME=dbagent

# AWS 설정 (선택사항)
AWS_DEFAULT_REGION=ap-northeast-2

# MCP 관련 환경 변수
UV_CACHE_DIR=/app/.uv-cache
PYTHONPATH=/app
PYTHONUNBUFFERED=1

# 애플리케이션 설정
SECRET_KEY=supersecretkey123
DEBUG=false
ENVIRONMENT=docker

# MCP 서버 설정
MCP_SERVER_TIMEOUT=30
MCP_MAX_RETRIES=3